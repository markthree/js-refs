    "vite-plugin-vue-meta-layouts": "^0.4.3",
    "vitest": "^1.6.0",
    "vue": "^3.5.12",
    "vue-dark-switch": "^1.0.6",
    "vue-echarts": "^6.7.3",
    "vue-i18n": "^9.14.1",
    "vue-request": "2.0.4",
    "vue-router": "^4.4.5",
    "vue-toastification": "2.0.0-rc.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --cache --fix"
  },
  "resolutions": {
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec",
    "vue-demi": "npm:vue-demi@0.14.10"
  },
  "repository": {
    "url": "https://github.com/markthree/js-refs"
